{
  "name": "backend",
  "version": "0.0.1",
  "description": "Simple storage calculator backend",
  "engines": {
    "node": "^24.11.1"
  },
  "scripts": {
    "start": "nest start",
    "dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint .",
    "test": "jest",
    "test:ci": "jest --ci --passWithNoTests",
    "seed": "ts-node src/seed/seed.ts",
    "deps:check:major": "ncu --target latest",
    "deps:update:minor": "ncu -u --target minor && pnpm install",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/infra/postgres/migrations/Init -d src/infra/postgres/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/infra/postgres/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/infra/postgres/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/typeorm": "^11.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.8",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "npm-check-updates": "^19.3.1",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
